/*
 * This is a logi.CAD 3 template for the remote PLC object
 */

{
	CHANNEL RemoteChannel
		TCP
			ADDRESS := 0.1.1.1;
			PORT    := 1534;
		END_TCP
	END_CHANNEL
}

CONFIGURATION RemoteConfiguration
	RESOURCE remote ON BuiltInPlc { ON_CHANNEL := RemoteChannel }
		TASK DefaultTask(INTERVAL := TIME#500ms, PRIORITY := 38229);
		PROGRAM Program1 WITH DefaultTask :
			Program1;
	END_RESOURCE
END_CONFIGURATION
