/*
 * This is a logi.CAD 3 template for the remote PLC object
 */

{
	CHANNEL NTx86Channel
		TCP
			ADDRESS := 127.0.0.1;
			PORT    := 1534;
		END_TCP
	END_CHANNEL
}

CONFIGURATION NTx86Configuration
	RESOURCE ntx86 ON WindowsX86 { ON_CHANNEL := NTx86Channel }
		TASK DefaultTask(INTERVAL := TIME#500ms, PRIORITY := 38229);
		PROGRAM Program1 WITH DefaultTask :
			Program1;
	END_RESOURCE
END_CONFIGURATION
